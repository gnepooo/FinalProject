@model FinalProject.Models.GroupProject

<h2>Leave Group Project</h2>

<p>Are you sure you want to leave the group project <strong>@Model.Title</strong>?</p>

<form asp-action="Leave" asp-route-id="@Model.Id" method="post">
    <button type="submit" class="btn btn-danger">Leave Project</button>
    <a asp-action="Index">Cancel</a>
</form>
